#include <bits/stdc++.h>
using namespace std;

class Solution
{
    public:
    //Function to check if two strings are rotations of each other or not.
    bool areRotations(string s1,string s2)
    {
        // Your code here
        if(s1.size()!=s2.size())
            return false;
        s1+=s1;
        if(s1.find(s2)<s1.size())
            return true;
        return false;
    }
};

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s1;
        string s2;
        cin>>s1>>s2;
        Solution obj;
        cout<<obj.areRotations(s1,s2)<<endl;
    }
    return 0;
}
